<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
  <Id>52800FFF-BA6F-4D05-FFAF-EFBB84DBFFFF</Id>
  <Version>1.0</Version>
  <ProviderName>Microsoft</ProviderName>
  <DefaultLocale>en-us</DefaultLocale>
  <DisplayName DefaultValue="Agave APIs (ac)" />
  <Description DefaultValue="Interactive Tutorial for Outlook" />
  <IconUrl DefaultValue="https://osf-agave/rtm/dasau/BuggerWeb/bug.png" />
  <HighResolutionIconUrl DefaultValue="https://osf-agave/rtm/dasau/BuggerWeb/bug.png" />
  <SupportUrl DefaultValue="https://www.microsoft.com" />
  <AppDomains>
    <AppDomain>domain.com</AppDomain>
  </AppDomains>
  <!--Host is NOT manditory in Outlook however if specified it must contain Mailbox-->
  <!--What happens if Host is Workbook?-->
  <!--Mailbox is not case sensitive-->
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <!--What happens if the version supported is higher than supported by Exchange?-->
  <!--Skip over any manifest whose version or API requirements are higher than the min of (Exchange, Outlook client)-->
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set MinVersion="1.1" Name="Mailbox" />
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html" />
      </DesktopSettings>
      <TabletSettings>
        <SourceLocation DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html" />
      </TabletSettings>
      <PhoneSettings>
        <SourceLocation DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html" />
      </PhoneSettings>
    </Form>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html" />
        <RequestedHeight>350</RequestedHeight>
      </DesktopSettings>
      <TabletSettings>
        <SourceLocation DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html" />
        <RequestedHeight>350</RequestedHeight>
      </TabletSettings>
      <PhoneSettings>
        <SourceLocation DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html" />
      </PhoneSettings>
    </Form>
  </FormSettings>
  <!--Reconsent when permissions level increased-->
  <Permissions>ReadWriteMailbox</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" FormType="ReadOrEdit" ItemType="Message" />
    <Rule xsi:type="ItemIs" FormType="ReadOrEdit" ItemType="Appointment" />
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Requirements>
      <bt:Sets DefaultMinVersion="1.3">
        <bt:Set Name="Mailbox" />
      </bt:Sets>
    </Requirements>

    <Hosts>
      <Host xsi:type="MailHost">
        <DesktopFormFactor>
          <FunctionFile resid="residDesktopFuncUrl" />
          <ExtensionPoint xsi:type="MessageComposeCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="msgcompTabDefault.grp1">
                <Label resid="residMsgcompTab" />
                <Tooltip resid="residMsgcompTabTip" />

                <Control xsi:type="Button" id="msg.btn">
                  <Label resid="residAgaveApisButton" />
                  <Tooltip resid="residAgaveApisButtonTip" />
                  <Supertip>
                    <Title resid="residSuperTip" />
                    <Description resid="residSuperTip2" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="icon_24" />                          
                    <bt:Image size="32" resid="icon_64" />
                    <bt:Image size="80" resid="icon_64" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="residAgaveApisUrl" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
          <ExtensionPoint xsi:type="MessageReadCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="msgcompTabDefault2.grp1">
                <Label resid="residMsgcompTab" />
                <Tooltip resid="residMsgcompTabTip" />

                <Control xsi:type="Button" id="msg2.btn">
                  <Label resid="residAgaveApisButton" />
                  <Tooltip resid="residAgaveApisButtonTip" />
                  <Supertip>
                    <Title resid="residSuperTip" />
                    <Description resid="residSuperTip2" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="icon_24" />                          
                    <bt:Image size="32" resid="icon_64" />
                    <bt:Image size="80" resid="icon_64" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="residAgaveApisUrl" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
          <ExtensionPoint xsi:type="AppointmentOrganizerCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="aptcompTabDefault.grp1">
                <Label resid="residMsgcompTab" />
                <Tooltip resid="residMsgcompTabTip" />

                <Control xsi:type="Button" id="apt.btn">
                  <Label resid="residAgaveApisButton" />
                  <Tooltip resid="residAgaveApisButtonTip" />
                  <Supertip>
                    <Title resid="residSuperTip" />
                    <Description resid="residSuperTip2" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="icon_24" />
                    <bt:Image size="32" resid="icon_64" />
                    <bt:Image size="80" resid="icon_64" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="residAgaveApisUrl" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="icon_24" DefaultValue="https://osf-agave/apps/olegu/IT/images/InteractiveAPI32x32.png" >
        </bt:Image>
        <bt:Image id="icon_64" DefaultValue="https://osf-agave/apps/olegu/IT/images/InteractiveAPI96x96.png" >
        </bt:Image>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="residDesktopFuncUrl" DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html" >
        </bt:Url>
        <bt:Url id="residAgaveApisUrl" DefaultValue="https://osf-agave/apps/olegu/IT/AgaveAPIs.html">
        </bt:Url>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="residMsgcompTab" DefaultValue="Agave APIs">
        </bt:String>
        <bt:String id="residAgaveApisButton" DefaultValue="Agave APIs">
        </bt:String>
        <bt:String id="residSuperTip" DefaultValue="Open a taskpane to play with Agave APIs">
        </bt:String>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="residMsgcompTabTip" DefaultValue="Agave APIs">
        </bt:String>
        <bt:String id="residAgaveApisButtonTip" DefaultValue="Open a taskpane to play with APIs">
        </bt:String>
        <bt:String id="residSuperTip2" DefaultValue="Open a taskpane that will provide a set of API tutorials to play with.">
        </bt:String>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>

</OfficeApp>